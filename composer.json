{
  "name": "alirezasadeghian79/rahatsms",
  "description": "Modular send sms system for Laravel/PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alireza Sadeghian",
      "email": "alirezasadeghian52@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "rahatSms\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "rahatSms\\Providers\\SmsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}